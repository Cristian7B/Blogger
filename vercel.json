{
  "version": 2,
  "builds": [
    {
      "src": "Blogger/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Blogger/wsgi.py"
    }
  ],
  "buildCommand": "./build_files.sh"
}
